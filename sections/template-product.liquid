                  <div class="row">
                    {% for cs in custom_switcher_opslag %}
                      {% if cs.kleur == currentproductattributes.kleur %}
                        <a href="{% if cs.url != '' %}{{ cs.url }}{% else %}{{ canonical }}{% endif %}" class="storage col-2 col-lg-2">
