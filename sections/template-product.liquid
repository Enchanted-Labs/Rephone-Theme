                      <div class="item">
                        <div class="inner">
                          <img src="{{ image.thumb }}" class="thumb" title="{{ product.title }}" alt="{{ product.title }}" style="height: 38px;"/>
                        </div>
                      </div>
                    {% endfor %}
                  {% endif %}
                </div>
              </div>
            </div>
